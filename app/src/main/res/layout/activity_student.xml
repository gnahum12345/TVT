<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".Student"
    android:orientation="vertical"
    android:weightSum="1"
    android:id="@+id/student"
    android:background="#ffffffff"
    >

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/LinearLayoutPeriod1"
        android:weightSum="1">

        <ImageView
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:id="@+id/teacherpic1"
            android:layout_weight="0.03"
            android:scaleType="fitCenter" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="198dp"
            android:layout_height="match_parent"
            android:id="@+id/vertical1">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="1">

                <TextView
                    android:layout_width="78dp"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Period 1"
                    android:id="@+id/class1"
                    android:textColor="#ff000000"
                    android:textStyle="bold"
                    android:layout_weight="0.64" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="teacher:"
                    android:id="@+id/teacher1"
                    android:gravity="right"
                    android:typeface="normal" />
            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Google:"
                android:id="@+id/twitter1" />

        </LinearLayout>

        <TextView
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="A"
            android:id="@+id/period1"
            android:textSize="70dp"
            android:gravity="top|center_vertical|center_horizontal"
            android:layout_gravity="top|right"
            android:focusable="false" />

    </LinearLayout>


    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/LinearLayoutPeriod2"
        android:weightSum="1">

        <ImageView
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:id="@+id/teacherpic2"
            android:layout_weight="0.03"
            android:scaleType="fitCenter" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="198dp"
            android:layout_height="match_parent"
            android:id="@+id/vertical2">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="1">

                <TextView
                    android:layout_width="78dp"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Period 2"
                    android:id="@+id/class2"
                    android:textColor="#ff000000"
                    android:textStyle="bold"
                    android:layout_weight="0.64" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="teacher:"
                    android:id="@+id/teacher2"
                    android:gravity="right"
                    android:typeface="normal" />
            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Google:"
                android:id="@+id/twitter2" />

        </LinearLayout>

        <TextView
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="B"
            android:id="@+id/period2"
            android:textSize="70dp"
            android:gravity="top|center_vertical|center_horizontal"
            android:layout_gravity="top|right"
            android:focusable="false" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/LinearLayoutPeriod3"
        android:weightSum="1">

        <ImageView
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:id="@+id/teacherpic3"
            android:layout_weight="0.03"
            android:scaleType="fitCenter" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="198dp"
            android:layout_height="match_parent"
            android:id="@+id/vertical3">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="1">

                <TextView
                    android:layout_width="78dp"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Period 3"
                    android:id="@+id/class3"
                    android:textColor="#ff000000"
                    android:textStyle="bold"
                    android:layout_weight="0.64" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="teacher:"
                    android:id="@+id/teacher3"
                    android:gravity="right"
                    android:typeface="normal" />
            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Google:"
                android:id="@+id/twitter3" />

        </LinearLayout>

        <TextView
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="C"
            android:id="@+id/period3"
            android:textSize="70dp"
            android:gravity="top|center_vertical|center_horizontal"
            android:layout_gravity="top|right"
            android:focusable="false" />

    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/LinearLayoutPeriod4"
        android:weightSum="1">

        <ImageView
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:id="@+id/teacherpic4"
            android:layout_weight="0.03"
            android:scaleType="fitCenter" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="198dp"
            android:layout_height="match_parent"
            android:id="@+id/vertical4">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="1">

                <TextView
                    android:layout_width="78dp"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Period 4"
                    android:id="@+id/class4"
                    android:textColor="#ff000000"
                    android:textStyle="bold"
                    android:layout_weight="0.64" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="teacher:"
                    android:id="@+id/teacher4"
                    android:gravity="right"
                    android:typeface="normal" />
            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Google:"
                android:id="@+id/twitter4" />

        </LinearLayout>

        <TextView
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="D"
            android:id="@+id/period4"
            android:textSize="70dp"
            android:gravity="top|center_vertical|center_horizontal"
            android:layout_gravity="top|right"
            android:focusable="false" />

    </LinearLayout>

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="google"
        android:id="@+id/google"
        android:background="#ffffffff" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/Buttons"
        android:background="#ffffffff">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/netClass"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:src="@mipmap/netclass"
            android:layout_alignBottom="@+id/classes"
            android:background="#ffffffff" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/classes"
            android:src="@mipmap/classes"
            android:background="#ffffffff"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/classesLabel" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/suggestions"
            android:src="@mipmap/questions"
            android:background="#ffffffff"
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/Sports"
            android:layout_marginEnd="46dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Parasha:"
            android:id="@+id/Parasha"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:layout_toStartOf="@+id/suggestions" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Sports"
            android:src="@mipmap/sports"
            android:background="#ffffffff"
            android:visibility="visible"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Netclass"
            android:id="@+id/netclassLabel"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:textColor="#ff000000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Classes"
            android:id="@+id/classesLabel"
            android:textColor="#ff000000"
            android:layout_alignTop="@+id/suggestionsLabel"
            android:layout_alignEnd="@+id/Parasha"
            android:layout_marginEnd="49dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Suggestions"
            android:id="@+id/suggestionsLabel"
            android:textColor="#ff000000"
            android:layout_alignTop="@+id/netclassLabel"
            android:layout_toEndOf="@+id/classesLabel" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Sports"
            android:id="@+id/spor"
            android:textColor="#ff000000"
            android:layout_alignTop="@+id/suggestionsLabel"
            android:layout_alignEnd="@+id/Sports" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Free or For Sale"
            android:id="@+id/freeorsale"
            android:layout_alignTop="@+id/Parasha"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_toEndOf="@+id/Parasha"
            android:textColor="#ff000000"
            android:textSize="20dp" />
    </RelativeLayout>

</LinearLayout>
